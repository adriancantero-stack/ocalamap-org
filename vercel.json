{
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/index.html",
            "destination": "/",
            "permanent": true
        },
        {
            "source": "/map.html",
            "destination": "/map",
            "permanent": true
        },
        {
            "source": "/terms.html",
            "destination": "/terms",
            "permanent": true
        },
        {
            "source": "/privacy.html",
            "destination": "/privacy",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}